#include <stdio.h>

int main() {
    int dia, mes;

    printf("Ingrese el dia: ");
    scanf("%d", &dia);
    printf("Ingrese el mes (1-12): ");
    scanf("%d", &mes);

    if ((mes == 9 && dia >= 21) || (mes == 12 && dia < 21) || (mes > 9 && mes < 12))
        printf("Primavera\n");
    else if ((mes == 12 && dia >= 21) || (mes == 3 && dia < 21) || (mes > 12 || mes < 3))
        printf("Verano\n");
    else if ((mes == 3 && dia >= 21) || (mes == 6 && dia < 21) || (mes > 3 && mes < 6))
        printf("Otonio\n");
    else
        printf("Invierno\n");

    return 0;
}