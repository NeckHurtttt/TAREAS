#include <stdio.h>
#include <math.h>

int main() {
    int N, digito, pos = 0;
    int complemento = 0;

    printf("Ingrese un numero entero: ");
    scanf("%d", &N);

    while (N != 0) {
        digito = N % 10;
        complemento += (9 - digito) * pow(10, pos);
        pos++;
        N /= 10;
    }

    printf("El complemento a 9 es: %d\n", complemento);
    return 0;
}
