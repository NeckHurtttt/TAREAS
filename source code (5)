#include <stdio.h>

int main() {
    int N, Z = 0, digito;

    printf("Ingrese un numero entero: ");
    scanf("%d", &N);

    while (N != 0) {
        digito = N % 10;     
        Z = Z * 10 + digito; 
        N /= 10;             
    }

    printf("El numero invertido es: %d\n", Z);
    return 0;
}